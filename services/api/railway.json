{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "/services/api/Dockerfile",
    "watchPatterns": [
      "/services/api/**"
    ]
  },
  "deploy": {
    "healthcheckPath": "/meta/healthcheck",
    "healthcheckTimeout": 300
  }
}